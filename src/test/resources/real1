<script>
var viral_tools = {
	email : function(article_id){
		if ($("#fb_vl_email").hasClass("on")){
			// remove it, slide up the box.
			$("#fb_vl_email").removeClass("on");
			$("#fb_vl_email_dd").slideUp(250,function(){
				$("#ra_vt_dropdown_main_thankyou_vt"+viral_tools.article_id).hide();
				$("#ra_vt_dropdown_mid_vt"+viral_tools.article_id).show();
			});
		} else {
			// add it, slide down the box.
			$("#fb_vl_email").addClass("on")
			$("#fb_vl_email_dd").slideDown(250);
			$("#fb_vl_share").removeClass("on");
			$("#fb_vl_share_dd").hide();
			// note, if captcha code is not present, call init with article_id
			if ($("#emailthis_cap_expr_"+article_id).html() == null){
				viral_tools.init(article_id);
			}

		}
	},
	share : function(){
		if ($("#fb_vl_share").hasClass("on")){
			// remove it, slide up the box.
			$("#fb_vl_share").removeClass("on");
			$("#fb_vl_share_dd").slideUp();
		} else {
			// add it, slide down the box.
			$("#fb_vl_share").addClass("on")
			$("#fb_vl_share_dd").slideDown(250);
			$("#fb_vl_email").removeClass("on");
			$("#fb_vl_email_dd").hide();
		}
	},
	"init" : function(article_id){
		$.ajaxSetup({ cache: false });
		viral_tools.article_id = article_id;
		var emailthis_addy = "/email-this" + $("#emailthis_path").val();
		$.get(emailthis_addy,function(data){
			viral_tools.process_emailthis(data);
		});
	},

	
	
	"process_emailthis" : function(data){
		var mydata = data.split("<div id=\"preview-benchmark\"")[0];
		var myObject = eval('(' + mydata + ')');
		viral_tools.emailthis_object = myObject;
		/*	obj.save_success;
			obj.article_id;
			obj.logged_in;
			obj.friend_name;*/
		$("#emailthis_cap_expr"+viral_tools.article_id).html(viral_tools.emailthis_object.captcha_exp);
		$("#emailthis_cap_key").val(viral_tools.emailthis_object.captcha_key);
		$("#emailthis_name_vt"+viral_tools.article_id).val(viral_tools.emailthis_object.name);
		$("#emailthis_email_vt"+viral_tools.article_id).val(viral_tools.emailthis_object.email);
		$("#emailthis_friendemail_vt"+viral_tools.article_id).val(viral_tools.emailthis_object.friend_email);
		$("#emailthis_message_vt"+viral_tools.article_id).val(viral_tools.emailthis_object.message);
		
		if (viral_tools.emailthis_object.errorstring == "There was a problem with the email address used.  Please enter a valid email address.\n"){
			viral_tools.emailthis_object.errorstring = "There is an error in one or more of the e-mail addresses you have provided";
		}
		if (viral_tools.emailthis_object.errorstring == "Problem with CAPTCHA: Invalid answer for key"){
			viral_tools.emailthis_object.errorstring = "Please make sure your answer is entered correctly";
		}
		if (viral_tools.emailthis_object.errorstring == "Problem with CAPTCHA: "){
			viral_tools.emailthis_object.errorstring = "Please make sure your answer is entered correctly";
		}
		if (viral_tools.emailthis_object.errorstring == "There was a problem with in sending your email.  Please try again.\n"){
			viral_tools.emailthis_object.errorstring = "There is something wrong with one or more of your friends' email addresses that you typed in.";
		}

		
		$("#emailthis_errorstring_vt"+viral_tools.article_id).html(viral_tools.emailthis_object.errorstring);
		$("#emailthis_cap_input").val("");
		if (viral_tools.emailthis_object.errorstring != ""){
			if (!checkEmail(viral_tools.emailthis_object.email)){
			//	$("#emailthis_email").css("background-color","#FF6600");
			}
			if (!checkEmail(viral_tools.emailthis_object.friend_email)){
			//	$("#emailthis_friendemail").css("background-color","#FF6600");
			}
			//alert("[To Process] ERROR: "+viral_tools.emailthis_object.errorstring);
			$("#ra_vt_dropdown_mid_vt"+viral_tools.article_id).css("height","210px");
			$("#emailthis_errorstring_vt"+viral_tools.article_id).show();
		}
		if (viral_tools.emailthis_object.save_success == "1"){
			$("#ra_vt_dropdown_mid_vt"+viral_tools.article_id).fadeOut("fast",function(){$("#ra_vt_dropdown_main_thankyou_vt"+viral_tools.article_id).fadeIn("fast")});
		}
		$("#emailthis_name_vt"+viral_tools.article_id).removeAttr("disabled");
		$("#emailthis_email_vt"+viral_tools.article_id).removeAttr("disabled");
		$("#emailthis_friendemail_vt"+viral_tools.article_id).removeAttr("disabled");
		$("#emailthis_message_vt"+viral_tools.article_id).removeAttr("disabled");
		$("#article_toolbar_dropdown_main_email_loading_vt"+viral_tools.article_id).hide();
	},
	"article_id" : "",
	"emailthis_object" : {},
	"email_post" : function(article_id){
		viral_tools.article_id = article_id;
		// lets do a check if the name field is empty
		var namefield = $("#emailthis_name_vt"+article_id).val();
		if ((namefield.replace(/^\s+|\s+$/g, '')) === ""){
			$("#fb_vl_dropdown_mid").css("height","230px");
			$("#ra_vt_dropdown_mid_vt"+viral_tools.article_id).css("height","210px");
			$("#emailthis_errorstring_vt"+article_id).html("Please fill in your name").show();
			//alert("ERROR: Please fill in your name");
			return false;
		}
		if ($("#emailthis_email_vt"+article_id).val() == ""){
			$("#fb_vl_dropdown_mid").css("height","230px");
			$("#ra_vt_dropdown_mid_vt"+viral_tools.article_id).css("height","210px");
			$("#emailthis_errorstring_vt"+article_id).html("Please fill in your e-mail address").show().css("color","red");
			return false;
		}
		if (!checkEmail($("#emailthis_email_vt"+article_id).val())){
			$("#fb_vl_dropdown_mid").css("height","230px");
			$("#ra_vt_dropdown_mid_vt"+viral_tools.article_id).css("height","210px");
			$("#emailthis_errorstring_vt"+article_id).html("Your e-mail address as entered does not appear to be a correct e-mail address").show().css("color","red");
			return false;
		}
		if ($("#emailthis_cap_input_vt"+article_id).val() == ""){
			$("#fb_vl_dropdown_mid").css("height","230px");
			$("#ra_vt_dropdown_mid_vt"+viral_tools.article_id).css("height","210px");
			$("#emailthis_errorstring_vt"+article_id).html("Please fill in an answer").show().css("color","red");
			return false;
		}
		if ($("#emailthis_friendemail_vt"+viral_tools.article_id).val().replace(/^\s+|\s+$/g, '') === ""){
			$("#fb_vl_dropdown_mid").css("height","230px");
			$("#ra_vt_dropdown_mid_vt"+viral_tools.article_id).css("height","210px");
			$("#emailthis_errorstring_vt"+article_id).html("There is something wrong with one or more of your friends' email addresses that you typed in.").show().css("color","red");
			return false;
		}
		
		if ($("#emailthis_name_vt"+article_id).attr("disabled")){
			$h.console.warn("EMAIL THIS: post execution disabled");
			return false;
		}
		
		
		
		$("#emailthis_name_vt"+article_id).attr("disabled","disabled");
		$("#emailthis_email_vt"+article_id).attr("disabled","disabled");
		$("#emailthis_friendemail_vt"+article_id).attr("disabled","disabled");
		$("#emailthis_message_vt"+article_id).attr("disabled","disabled");
		$("#article_toolbar_dropdown_main_email_loading_vt"+article_id).show();
		viral_tools.emailthis_object.name = $("#emailthis_name_vt"+article_id).val();
		viral_tools.emailthis_object.email = $("#emailthis_email_vt"+article_id).val();
		viral_tools.emailthis_object.friend_email = $("#emailthis_friendemail_vt"+article_id).val();
		viral_tools.emailthis_object.message = $("#emailthis_message_vt"+article_id).val();
		viral_tools.emailthis_object.article_id = article_id;
		viral_tools.emailthis_object.captcha_answer = $("#emailthis_cap_input_vt"+article_id).val();
		viral_tools.emailthis_object.captcha_key = $("#emailthis_cap_key").val();
		
		$.post("/email-this/email-this", { "article_id" : viral_tools.emailthis_object.article_id, "captcha_answer" : viral_tools.emailthis_object.captcha_answer, "captcha_key" : viral_tools.emailthis_object.captcha_key, "name" : viral_tools.emailthis_object.name, "email" : viral_tools.emailthis_object.email, "friend_email" : viral_tools.emailthis_object.friend_email, "friend_name" : viral_tools.emailthis_object.friend_name, "message" : viral_tools.emailthis_object.message },function(data){viral_tools.process_emailthis(data);});
	},
	"bookmark" : function(name){
		switch(name){
			case "digg":
				var url = document.location;
				window.open("http://digg.com/submit?url="+escape(url));
				break;
			case "facebook":
				var d=document,f='http://www.facebook.com/share',l=d.location,e=encodeURIComponent,p='.php?src=bm&v=4&i=1239291108&u='+e(l.href)+'&t='+e(d.title);
				1;
				try{if (!/^(.*\.)?facebook\.[^.]*$/.test(l.host))throw(0);
				share_internal_bookmarklet(p)}catch(z) {a=function() {if (!window.open(f+'r'+p,'sharer','toolbar=0,status=0,resizable=1,width=626,height=436'))l.href=f+p};
				if (/Firefox/.test(navigator.userAgent))setTimeout(a,0);else{a()}}void(0)
				break;
			case "newsvine":
				window.open('http://www.newsvine.com/_wine/save?popoff=1&u='+document.location+'','newsvine','toolbar=no,width=590,height=600,resizable=yes,scrollbars=yes');
				break;
			case "delicious":
				f='http://delicious.com/save?url='+encodeURIComponent(window.location.href)+'&title='+encodeURIComponent(document.title)+'&v=5&';
				a=function(){if(!window.open(f+'noui=1&jump=doclose','deliciousuiv5','location=yes,links=no,scrollbars=no,toolbar=no,width=550,height=550'))location.href=f+'jump=yes'};
				if(/Firefox/.test(navigator.userAgent)){setTimeout(a,0)}else{a()}
				break;
			case "magnolia":
				window.open("http://ma.gnolia.com/");
				break;
			case "reddit":
				window.open('http://www.reddit.com/submit?url='+encodeURIComponent(location.href)+'&title='+encodeURIComponent(document.title));
				break;
			case "mslive":
				t=document.title;
				u=escape(location.href);
				window.open('https://favorites.live.com/quickadd.aspx?marklet=1&mkt=en-us&url='+u+'&title='+t+'&top=0','live','toolbar=no,width=590,height=600,resizable=yes,scrollbars=yes');
				break;
			case "buzz":
				var yahooBuzzArticleHeadline = document.title;
				//var yahooBuzzArticleSummary = '';
				var yahooBuzzArticleCategory = "health";
				var yahooBuzzArticleType = "text";
				var yahooBuzzArticleId = window.location.href;
				window.open("http://buzz.yahoo.com/buzz?targetUrl="+encodeURIComponent(location.href));
				break;
			case "google":
				var a=window,b=document,c=encodeURIComponent,d=a.open("http://www.google.com/bookmarks/mark?op=edit&output=popup&bkmk="+c(b.location)+"&title="+c(b.title),"bkmk_popup","left="+((a.screenX||a.screenLeft)+10)+",top="+((a.screenY||a.screenTop)+10)+",height=420px,width=550px,resizable=1,alwaysRaised=1");a.setTimeout(function(){d.focus()},300)
				break;
			case "popsugar":
				var url = document.location;
				window.open("http://teamsugar.com/node/add/userlink?t=fabfind&url="+encodeURIComponent(location.href)+"&body="+""+"&title="+encodeURIComponent(document.title));
				//http://teamsugar.com/node/add/userlink?t=fabfind&url=http://www.cosmogirl.com/blog/blog/terrible-tattoos%3Fsrc%3Dteamsugar&body=What%20word%20comes%20to%20mind%20when%20you%20look%20at%20these%20terrible%20tattoos?&title=Together%20Forever
				break;
			case "stumbleupon":
				window.open("http://www.stumbleupon.com/submit?url="+encodeURIComponent(location.href)+"&title="+encodeURIComponent(document.title));
				break;
			default:
				alert("UNKNOWN BOOKMARK");
				break;
		}
	},
	"print_this" : function(section_path,url_name){
		$("#fb_vl_email_dd").hide();
		$("#fb_vl_share_dd").hide();
		$("#fb_vl_email").removeClass("on");
		$("#fb_vl_share").removeClass("on");
		window.open ("/print-this"+section_path+url_name+"?page=all");
	}
}

function checkEmail(fieldValue){
	if(fieldValue != ""){
		var atSymbol = 0
		for(var a = 0; a < fieldValue.length; a++){
			if(fieldValue.charAt(a) == "@"){
				atSymbol++
			}
		}
		if(atSymbol > 1){return false}
		if(atSymbol == 1 && fieldValue.charAt(0) != "@"){
			var period = fieldValue.indexOf(".",fieldValue.indexOf("@")+2)
			var twoPeriods = (fieldValue.charAt((period+1)) == ".") ? true : false
			if(period == -1 || twoPeriods || fieldValue.length < period + 2 || fieldValue.charAt(fieldValue.length-1)=="."){
				return false
			}
		}else{return false}
	}else{return false}
	return true
}



