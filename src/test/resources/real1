	$("[session='toregister']").hide();
	$("[session='toauth']").html('<a href="'+logoutURL+'" onclick="return deleteRAAPCookies();">Sign Out</a>');
	$("[session='toauth']").show();
	$("[session='toaccount']").show();
	var raap_member_id = readCookie("raap_member_id");
	var realage_timeout = readCookie("realage_timeout");
	if (raap_member_id && realage_timeout) {
		if (raap_member_id.length > 0 && realage_timeout.length > 0) {
			$("[loggedin]").attr("href",function (i, val) { return $(this).attr("loggedin"); });
		}
	}
});
