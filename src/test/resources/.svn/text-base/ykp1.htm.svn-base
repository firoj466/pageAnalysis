//留言投票开始
function LeaveWordtoupiao(leaveWordId,tpType)
{
    $.post("21Commentary/AjaxPage/CommentaryAdmin.ashx",{type:'LeaveWordtoupiao',tpType:tpType,leaveWordId:leaveWordId},toupiaoBackFun);
}
function toupiaoBackFun(htm)
{
    if(htm=="false")
    { 
        alert("你已投过票，每天只能投一次");
    }
    else
    {
        var arr=htm.split("|");
        var leaveWordId=arr[0];//留言编号
        var vsupport=arr[1];//获取支持数
        var voppose=arr[2];//获取反对数
        document.getElementById("support"+leaveWordId).innerHTML="("+vsupport+")";
        document.getElementById("oppose"+leaveWordId).innerHTML="("+voppose+")";
    }
}


